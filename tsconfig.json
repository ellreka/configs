{
  "compilerOptions": {
    "strict": true,
    "target": "ES5",
    "module": "es2015",
    "lib": ["es2019", "dom"],
    "esModuleInterop": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "forceConsistentCasingInFileNames": true,
    "sourceMap": true,
    "allowJs": true,
    "declaration": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
  }
}
